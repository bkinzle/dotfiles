{
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
    "git.autofetch": true,
    "git.confirmSync": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "terminal.integrated.scrollback": 10000000,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "explorer.compactFolders": false,
    "git.allowForcePush": true,
    "workbench.colorTheme": "Default Dark+",
    "search.exclude": {
        "**/.venv": true
    },
    "notebook.output.textLineLimit": 2000,
    "notebook.lineNumbers": "on",
    "[jinja-sql]": {
        "editor.tabSize": 2
    },
    "githubPullRequests.pullBranch": "never",
    "files.associations": {
        "*.ndjson": "jsonl",
        "*.hcl": "terraform"
    },
    "remote.autoForwardPortsFallback": 0,
    "python.interpreter.infoVisibility": "always",
    "jupyter.askForKernelRestart": false,
    "redhat.telemetry.enabled": false,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "vs-kubernetes": {
        "disable-linters": [
            "resource-limits"
        ],
        "vs-kubernetes.namespace": "",
        "vs-kubernetes.kubectl-path": "",
        "vs-kubernetes.helm-path": "",
        "vs-kubernetes.minikube-path": "",
        "vs-kubernetes.kubectlVersioning": "user-provided",
        "vs-kubernetes.outputFormat": "yaml",
        "vs-kubernetes.kubeconfig": "",
        "vs-kubernetes.knownKubeconfigs": [],
        "vs-kubernetes.autoCleanupOnDebugTerminate": false,
        "vs-kubernetes.nodejs-autodetect-remote-root": true,
        "vs-kubernetes.nodejs-remote-root": "",
        "vs-kubernetes.nodejs-debug-port": 9229,
        "vs-kubernetes.dotnet-vsdbg-path": "~/vsdbg/vsdbg",
        "vs-kubernetes.local-tunnel-debug-provider": "",
        "checkForMinikubeUpgrade": true,
        "imageBuildTool": "Docker"
    },
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "file:/**/*.parquet": "jupyter-data-wrangler"
    },
    "editor.tabSize": 2,
    "diffEditor.ignoreTrimWhitespace": false,
    "github.copilot.nextEditSuggestions.enabled": true,
    "editor.fontFamily": "Hack Nerd Font Mono, Menlo, Monaco, 'Courier New', monospace",
    "githubLocalActions.dockerDesktopPath": "/Applications/Docker.app",
    "docker.extension.enableComposeLanguageServer": false,
    "cSpell.customDictionaries": {
        "custom-dictionary-user": {
            "name": "custom-dictionary-user",
            "path": "~/dotfiles/.config/cspell/custom-dictionary-user.txt",
            "addWords": true,
            "scope": "user"
        }
    },
    "debug.internalConsoleOptions": "openOnSessionStart",
    "terminal.integrated.suggest.enabled": false,
    "claudeCode.preferredLocation": "panel",
}
